{
  "name": "punchlist",
  "version": "1.0.0",
  "description": "Punch List for Tasks",
  "main": "dist/punchlist.js",
  "scripts": {
    "build": "npm run buildExample && npm run buildDist ",
    "buildExample": "copyfiles -u 1 src/punchlist.js example/js && copyfiles -u 1 src/punchlist.css example/css",
    "buildDist": "copyfiles -u 1 src/* dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "jquery": "^3.3.1"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "author": {
    "name": "gabwagner",
    "url": "https://github.com/gabwagner/punchList/"
  },
  "bugs": {
    "url": "https://github.com/gabwagner/punchList/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.1.0"
  }
}
